<application>
  <component name="GetterTemplates">
    <option name="defaultTempalteName" value="Android" />
    <option name="templates">
      <list>
        <TemplateResource>
          <option name="fileName" value="Android" />
          <option name="template" value="#if($field.modifierStatic)&#10;static ##&#10;#end&#10;$field.type ##&#10;#set($name = $StringUtil.capitalizeWithJavaBeanConvention($StringUtil.sanitizeJavaIdentifier($helper.getPropertyName($field, $project))))&#10;#if ($field.boolean &amp;&amp; $field.primitive &amp;&amp; $field.public)&#10;  is##&#10;#else&#10;  get##&#10;#end&#10;${name}() {&#10;  return $field.name;&#10;}" />
        </TemplateResource>
      </list>
    </option>
  </component>
</application>